{
  "name": "nexora",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web && powershell -Command \"Start-Process http://localhost:8081\"",
    "web:clean": "expo start --web -c && powershell -Command \"Start-Process http://localhost:8081\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start:tunnel": "expo start --tunnel",
    "clean": "npx rimraf node_modules package-lock.json .expo .expo-shared && npx rimraf android/build ios/build webapp/node_modules webapp/package-lock.json",
    "reset": "npm run clean && npm install && expo start -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "perf": "node scripts/perfSample.cjs",
    "sync:drain": "node scripts/syncDrain.cjs",
    "version:patch": "node scripts/bumpVersion.cjs patch",
    "version:minor": "node scripts/bumpVersion.cjs minor",
    "version:major": "node scripts/bumpVersion.cjs major",
    "prepare": "husky install",
    "postinstall": "husky install && node scripts/setupHusky.mjs"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "dayjs": "^1.11.13",
    "expo": "^53.0.0",
    "expo-asset": "~11.1.7",
    "expo-crypto": "^14.1.5",
    "expo-font": "~13.3.2",
    "expo-linear-gradient": "~14.1.5",
    "expo-location": "~18.0.0",
    "leaflet": "^1.9.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-leaflet": "^5.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "^1.3.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.2.7",
    "react-test-renderer": "19.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "@types/react-native"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  }
}
